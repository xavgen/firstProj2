<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/drawable_shape_40"
    android:padding="0dp"
    tools:context=".FirstFragment">
<!--    android:background="#0b0f1f"-->

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="0dp">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="760dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.492"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0">


            <ImageView
                android:id="@+id/addEvent"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_gravity="bottom|end"
                android:background="@drawable/drawable_shape_1"
                android:gravity="center"
                android:orientation="horizontal"
                android:scaleType="centerInside"
                android:src="@drawable/drawable_image"
                android:translationX="-15dp"
                android:translationY="-80dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:layout_gravity="bottom|start"
                android:layout_marginTop="6dp"
                android:background="@drawable/drawable_shape_2"
                android:gravity="start|top"
                android:orientation="horizontal"
                android:paddingTop="3.33dp">

                <LinearLayout
                    android:id="@+id/today"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:clickable="true"
                    android:clipToPadding="false"
                    android:focusable="true"
                    android:gravity="center_horizontal|top"
                    android:orientation="vertical"
                    android:scaleType="fitXY">

                    <ImageView
                        android:layout_width="60dp"
                        android:layout_height="40dp"
                        android:layout_marginTop="6dp"
                        android:scaleType="centerInside"
                        android:src="@drawable/drawable_image_1" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="15dp"
                        android:gravity="center_horizontal|center_vertical"
                        android:includeFontPadding="false"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:text="Today"
                        android:textColor="#FF80B3FF"
                        android:textSize="10sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/schedule"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:clickable="true"
                    android:clipToPadding="false"
                    android:focusable="true"
                    android:gravity="center_horizontal|top"
                    android:orientation="vertical"
                    android:scaleType="fitXY">

                    <ImageView
                        android:layout_width="60dp"
                        android:layout_height="40dp"
                        android:layout_marginTop="6dp"
                        android:scaleType="centerInside"
                        android:src="@drawable/drawable_image_2" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="15dp"
                        android:gravity="center_horizontal|center_vertical"
                        android:includeFontPadding="false"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:text="Schedule"
                        android:textColor="#FFEBEBF5"
                        android:textSize="10dp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/toDo"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:clickable="true"
                    android:clipToPadding="false"
                    android:focusable="true"
                    android:gravity="center_horizontal|top"
                    android:orientation="vertical"
                    android:paddingTop="1dp"
                    android:paddingBottom="0.33dp"
                    android:scaleType="fitXY">

                    <ImageView
                        android:layout_width="60dp"
                        android:layout_height="40dp"
                        android:layout_marginTop="6dp"
                        android:scaleType="centerInside"
                        android:src="@drawable/drawable_image_3" />

